<div class= 'row'>
<div class="col s12">
<h3 class= 'align center'>Students</h3>
</div>
</div>
<div class="row">
<% @students.each do |student| %>
    <div class="col s4  align center">
      <div class="card hoverable">
        <div class="card-image">
         <%= image_tag("girl2") %> 
          <span class="card-title yellow accent-3"><%= student.id %></span>
        </div>
        <div class="card-content">
          <p><%= student.name %></p>
          <p><%= student.email %></p>
        </div>
        <div class="card-action yellow accent-3">
          <%= link_to 'View Profile', student_path(student) %>
        </div>
      </div>
    </div>
    <% end %>
  </div>

